{
  "name": "discord-to-fvtt",
  "scripts": {
    "build": "yarn prebuild && esbuild src/index.js --bundle --keep-names --minify --sourcemap --outdir=dist/scripts",
    "dev": "yarn prebuild && esbuild src/index.js --bundle --keep-names --watch --sourcemap --outdir=dist/scripts",
    "prebuild": "mkdir -p dist/scripts && cp -R module.json static/ styles/ dist/"
  },
  "dependencies": {
    "discord.js": "^14.15.3"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2"
  },
  "packageManager": "yarn@4.4.0",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
